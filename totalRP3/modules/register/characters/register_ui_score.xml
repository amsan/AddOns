	<!-- Register Score panel -->
	<Frame name="TRP3_RegisterScoreTemplate" virtual="true">
		<Backdrop edgeFile="Interface\Tooltips\UI-Tooltip-Border">
			<EdgeSize val="24"/>
		</Backdrop>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="background" file="Interface\FrameGeneral\UI-Background-Marble" setAllPoints="true">
					<Anchors>
						<Anchor point="TOPRIGHT" x="-5" y="-2"/>
						<Anchor point="BOTTOMRIGHT" x="-5" y="5"/>
						<Anchor point="LEFT" x="5" y="0"/>
					</Anchors>
					<!--TexCoords left="0.07" right="1" top="0" bottom="0.975" /-->
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture parentKey="logo" file="Interface\AddOns\totalRP3\resources\RIO.tga">
					<Size x="256" y="128"/>
					<Anchors>
						<Anchor point="TOP" x="0" y="20"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
</Ui>